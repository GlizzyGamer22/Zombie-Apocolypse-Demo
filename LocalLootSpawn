function WeaponLootCheck(wepgroup)
	for i = 1, #wepgroup do
		local distancecheck = 0
		for m = 1, #playerlist do 
			if playerlist[m].Character then
				if playerlist[m].Character:findFirstChild("Torso") then
					if (playerlist[m].Character.Torso.Position - wepgroup[i].Position).magnitude < spawndistance and wepgroup[i]:findFirstChild("Check") == nil then
						ting = math.random(1,2800)
						model = nil
						if ting >= 10 and ting < 60 then
							model = backpack:findFirstChild("Shotgun"):clone()
						elseif ting >= 70 and ting < 130 then
							model = backpack:findFirstChild("M1911"):clone()
						elseif ting >= 140 and ting < 180 then
							model = backpack:findFirstChild("Maverick"):clone()
						elseif ting >= 230 and ting < 275 then
							model = backpack:findFirstChild("Mosin-Nagant"):clone()
						elseif ting >= 280 and ting < 302 then
							model = backpack:findFirstChild("M1 Garand"):clone()
						elseif ting >= 310 and ting < 380 then
							model = backpack:findFirstChild("Revolver"):clone()
						elseif ting >= 380 and ting < 440 then
							model = backpack:findFirstChild("Hatchet"):clone()
						elseif ting >= 460 and ting < 530 then
							model = backpack:findFirstChild("Crowbar"):clone()
						elseif ting >= 560 and ting < 610 then
							model = backpack:findFirstChild("CZ75"):clone()
						elseif ting >= 630 and ting < 660 then
							model = backpack:findFirstChild("Enfield"):clone()
						elseif ting >= 660 and ting < 740 then
							model = backpack:findFirstChild("Makarov"):clone()
						elseif ting >= 740 and ting < 751 then
							model = backpack:findFirstChild("SKS"):clone()
						elseif ting >= 758 and ting < 770 then
							model = backpack:findFirstChild("Fedorov"):clone()
						elseif ting >= 770 and ting < 779 then
							model = backpack:findFirstChild("PPSH"):clone()
						elseif ting >= 780 and ting < 800 then
							model = backpack:findFirstChild("Auto-5"):clone()
						elseif ting >= 800 and ting < 822 then
							model = backpack:findFirstChild("M3"):clone()
						elseif ting >= 830 and ting < 870 then
							model = backpack:findFirstChild("Model 459"):clone()
						end 
						if model then
							local g = model:GetChildren()
							for d = 1, #g do
								if g[d].className == "Model" then
									g[d]:TranslateBy((wepgroup[i].Position - g[d].Head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
									local ammo = nil
									if model.Name == "Shotgun" then
										ammo = backpack.ShotgunAmmo
									elseif model.Name == "M1911" then
										ammo = backpack.M1911Ammo
									elseif model.Name == "Mosin-Nagant" then
										ammo = backpack.NagantAmmo
									elseif model.Name == "M1 Garand" then
										ammo = backpack.GarandAmmo
									elseif model.Name == "Maverick" then
										ammo = backpack.MaverickAmmo
									elseif model.Name == "Revolver" then
										ammo = backpack.RevolverAmmo
									elseif model.Name == "CZ75" then
										ammo = backpack.CZ75Ammo
									elseif model.Name == "Enfield" then
										ammo = backpack.EnfieldAmmo
									elseif model.Name == "Fedorov" then
										ammo = backpack.FedorovAmmo
									elseif model.Name == "SKS" then
										ammo = backpack.SKSAmmo
									elseif model.Name == "PPSH" then
										ammo = backpack.PPSHAmmo
									elseif model.Name == "Makarov" then
										ammo = backpack.MakarovAmmo
									elseif model.Name == "M3" then
										ammo = backpack.M3Ammo30
									elseif model.Name == "Model 459" then
										ammo = backpack.Model459Ammo14
									elseif model.Name == "Auto-5" then
										ammo = backpack.MaverickAmmo
									end 
									
									if ammo then
										local bing = math.random(1,3)
										local ding = 0
										repeat 
											ding = ding + 1
											ammonew = ammo:clone()
											local head = ammonew:findFirstChild("Head", true)
											head.Parent:TranslateBy((wepgroup[i].Position - head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
											ammonew.Parent = Game.Workspace.Loot
										until bing == ding
									end
									
									local m = g[d]:GetChildren()
									for x = 1, #m do
										if m[x].className == "Part" then
											m[x].Velocity = Vector3.new(0,0,0)
										end
									end
								end
							end 
							model.Parent = Game.Workspace.Loot
						end
						placer = Instance.new("IntValue")
						placer.Name = "Check"
						placer.Value = 20
						placer.Parent = wepgroup[i]
					end 
					if (playerlist[m].Character.Torso.Position - wepgroup[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and wepgroup[i]:findFirstChild("Check") then
			wepgroup[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

function MilitaryWLootCheck()
	for i = 1, #_G.mwep do
		local distancecheck = 0
		for m = 1, #playerlist do 
			if playerlist[m].Character then
				if playerlist[m].Character:findFirstChild("Torso") then
					if (playerlist[m].Character.Torso.Position - _G.mwep[i].Position).magnitude < spawndistance and _G.mwep[i]:findFirstChild("Check") == nil then
						ting = math.random(1,2700)
						model = nil
						
						if ting >= 60 and ting < 84 then -- AK-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 40 then
								model = backpack:findFirstChild("AKS-74U"):clone()
							elseif tiert >= 40 and tiert < 80 then
								model = backpack:findFirstChild("AK-74"):clone()
							elseif tiert >= 80 and tiert < 100 then
								model = backpack:findFirstChild("AN-94"):clone()
							end
						
						elseif ting >= 90 and ting < 108 then -- AK47-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 25 then
								model = backpack:findFirstChild("RPK"):clone()
							elseif tiert >= 25 and tiert < 50 then
								model = backpack:findFirstChild("AK-47"):clone()
							elseif tiert >= 50 and tiert < 65 then
								model = backpack:findFirstChild("AKM"):clone()
							elseif tiert >= 65 and tiert < 80 then
								model = backpack:findFirstChild("OTs-14"):clone()
							elseif tiert >= 80 and tiert < 90 then
								model = backpack:findFirstChild("AK-12"):clone()
							elseif tiert >= 90 and tiert < 100 then
								model = backpack:findFirstChild("AK-104"):clone()
							end
						
						elseif ting >= 110 and ting < 123 then -- M14-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 30 then
								model = backpack:findFirstChild("HK21"):clone()
							elseif tiert >= 30 and tiert < 50 then
								model = backpack:findFirstChild("G3"):clone()
							elseif tiert >= 50 and tiert < 65 then
								model = backpack:findFirstChild("HK417"):clone()
							elseif tiert >= 65 and tiert < 80 then
								model = backpack:findFirstChild("M14"):clone()
							elseif tiert >= 80 and tiert < 90 then
								model = backpack:findFirstChild("FAL"):clone()
							elseif tiert >= 90 and tiert < 100 then
								model = backpack:findFirstChild("Mk 17"):clone()
							end
						
						elseif ting >= 200 and ting < 221 then -- M4-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 40 then
								model = backpack:findFirstChild("M4A1"):clone()
							elseif tiert >= 40 and tiert < 75 then
								model = backpack:findFirstChild("SCAR-L"):clone()
							elseif tiert >= 75 and tiert < 100 then
								model = backpack:findFirstChild("G36K"):clone()
							end
						
						elseif ting >= 230 and ting < 320 then -- TEC9-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 50 then
								model = backpack:findFirstChild("TEC-9"):clone()
							elseif tiert >= 50 and tiert < 80 then
								model = backpack:findFirstChild("Uzi"):clone()
							elseif tiert >= 80 and tiert < 100 then
								model = backpack:findFirstChild("CBJ-MS"):clone()
							end
						
						elseif ting >= 320 and ting < 450 then -- Handgun-Type
							local tiert = math.random(1,100)
							if tiert >= 1 and tiert < 25 then
								model = backpack:findFirstChild("M93R"):clone()
							elseif tiert >= 25 and tiert < 50 then
								model = backpack:findFirstChild("G18"):clone()
							elseif tiert >= 50 and tiert < 65 then
								model = backpack:findFirstChild("M9"):clone()
							elseif tiert >= 65 and tiert < 80 then
								model = backpack:findFirstChild("USP45"):clone()
							elseif tiert >= 80 and tiert < 90 then
								model = backpack:findFirstChild("G37"):clone()
							elseif tiert >= 90 and tiert < 100 then
								model = backpack:findFirstChild("Mk 23"):clone()
							end

						elseif ting >= 530 and ting < 533 then
							model = backpack:findFirstChild("Mk 48"):clone()
						elseif ting >= 580 and ting < 600 then
							model = backpack:findFirstChild("MP5"):clone()
						elseif ting >= 620 and ting < 650 then
							model = backpack:findFirstChild("M870"):clone()
						elseif ting >= 710 and ting < 714 then
							model = backpack:findFirstChild("M249"):clone()
						elseif ting >= 750 and ting < 775 then
							model = backpack:findFirstChild("PP-19"):clone()
						elseif ting >= 780 and ting < 800 then
							model = backpack:findFirstChild("M1014"):clone()
						elseif ting >= 800 and ting < 840 then
							model = backpack:findFirstChild("Knife"):clone()
						elseif ting >= 840 and ting < 848 then
							model = backpack:findFirstChild("Sabre"):clone()
						end 
						if model then
							local g = model:GetChildren()
							for d = 1, #g do
								if g[d].className == "Model" then
									g[d]:TranslateBy((_G.mwep[i].Position - g[d].Head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
									local ammo = nil
									if model.Name == "AK-47" or model.Name == "AKM" or model.Name == "AK-12" or model.Name == "AK-104" or model.Name == "RPK" or model.Name == "OTs-14" then
										ammo = backpack.AK47Ammo30
									elseif model.Name == "M9" or model.Name == "G18" or model.Name == "M93R" then
										ammo = backpack.M9Ammo17
									elseif model.Name == "TEC-9" or model.Name == "CBJ-MS" or model.Name == "Uzi" then
										ammo = backpack.TEC9Ammo20
									elseif model.Name == "M4A1" or model.Name == "G36K" or model.Name == "SCAR-L" then
										ammo = backpack.STANAGAmmo30
									elseif model.Name == "USP45" or model.Name == "G37" or model.Name == "Mk 23" then
										ammo = backpack.USP45Ammo
									elseif model.Name == "M14" or model.Name == "HK21" or model.Name == "HK417" or model.Name == "G3" or model.Name == "FAL" or model.Name == "Mk 17" then
										ammo = backpack.M14Ammo20
									elseif model.Name == "AK-74" or model.Name == "AN-94" or model.Name == "AKS-74U" then
										ammo = backpack.AKAmmo30
									elseif model.Name == "M870" or model.Name == "M1014" then
										ammo = backpack.M870Ammo
									elseif model.Name == "Mk 48" then
										ammo = backpack.Mk48Ammo
									elseif model.Name == "MP5" then
										ammo = backpack.MP5Ammo
									elseif model.Name == "M249" then
										ammo = backpack.M249Ammo100
									elseif model.Name == "PP-19" then
										ammo = backpack.PP19Ammo64
									end
									
									if ammo then
										local bing = math.random(1,3)
										local ding = 0
										repeat 
											ding = ding + 1
											ammonew = ammo:clone()
											local head = ammonew:findFirstChild("Head", true)
											head.Parent:TranslateBy((_G.mwep[i].Position - head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
											ammonew.Parent = Game.Workspace.Loot
										until bing == ding
									end
									
									local m = g[d]:GetChildren()
									for x = 1, #m do
										if m[x].className == "Part" then
											m[x].Velocity = Vector3.new(0,0,0)
										end
									end
								end
							end 
							model.Parent = Game.Workspace.Loot
						end
						placer = Instance.new("IntValue")
						placer.Name = "Check"
						placer.Value = 20
						placer.Parent = _G.mwep[i]
					end 
					if (playerlist[m].Character.Torso.Position - _G.mwep[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and _G.mwep[i]:findFirstChild("Check") then
			_G.mwep[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

Game.Workspace.Loot.ChildAdded:connect(Anchor)

while true do
	wait(1)
	playerlist = _G.PlayerList
	GenericLootCheck(_G.gengroup1)
	wait(1)
	MilitaryWLootCheck()
	wait(1)
	GenericLootCheck(_G.gengroup2)
	wait(1)
	BuildingMaterialLootCheck()
	wait(1)
	WeaponLootCheck(_G.wepgroup1)
	wait(1)
	MedicalLootCheck()
	wait(1)
	GenericLootCheck(_G.gengroup3)
	wait(1)
	MilitaryGLootCheck()
	wait(1)
	WeaponLootCheck(_G.wepgroup2)
	wait(1)
	VehiclePartLootCheck()
	wait(1)
	DespawnLoot()
end 
