					end 
					if (playerlist[m].Character.Torso.Position - gengroup[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and gengroup[i]:findFirstChild("Check") then
			gengroup[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

function MedicalLootCheck()
	for i = 1, #_G.med do
		local distancecheck = 0
		for m = 1, #playerlist do 
			if playerlist[m].Character then
				if playerlist[m].Character:findFirstChild("Torso") then
					if (playerlist[m].Character.Torso.Position - _G.med[i].Position).magnitude < spawndistance and _G.med[i]:findFirstChild("Check") == nil then
						repeat
						ting = math.random(1,100)
						model = nil
						if ting >= 1 and ting < 3 then
							model = backpack.Beans:clone()
						elseif ting >= 3 and ting < 5 then
							model = backpack.Tuna:clone()
						elseif ting >= 5 and ting < 7 then
							model = backpack.Pasta:clone()
						elseif ting >= 7 and ting < 9 then
							model = backpack.Spam:clone()
						elseif ting >= 9 and ting < 11 then
							model = backpack.TomatoSoup:clone()
						elseif ting >= 11 and ting < 13 then
							model = backpack.Vegetables:clone()
						elseif ting >= 13 and ting < 15 then
							model = backpack.BeefStew:clone()
						elseif ting >= 15 and ting < 17 then
							model = backpack.ChickenSoup:clone()
						elseif ting >= 17 and ting < 20 then
							model = backpack.SodaCoke:clone()
						elseif ting >= 20 and ting < 23 then
							model = backpack.SodaPepsi:clone()
						elseif ting >= 23 and ting < 26 then
							model = backpack.SodaDew:clone()
						elseif ting >= 26 and ting < 29 then
							model = backpack.SodaCrush:clone()
						elseif ting >= 29 and ting < 31 then
							model = backpack.SodaMoxie:clone()
						elseif ting >= 31 and ting < 33 then
							model = backpack.SodaSprite:clone()
						elseif ting >= 33 and ting < 34 then
							model = backpack.SodaPepper:clone()
						elseif ting >= 34 and ting < 52 then
							model = backpack.Painkillers:clone()
						elseif ting >= 52 and ting < 100 then
							model = backpack.BloodBag:clone()
						end 
						if model then
							local g = model:GetChildren()
							for d = 1, #g do
								if g[d].className == "Model" then
									g[d]:TranslateBy((_G.med[i].Position - g[d].Head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
									local m = g[d]:GetChildren()
									for x = 1, #m do
										if m[x].className == "Part" then
											m[x].Velocity = Vector3.new(0,0,0)
										end
									end
								end
							end 
							model.Parent = Game.Workspace.Loot
						end
						zing = zing + 1
						until zing == 4
						zing = 0
						placer = Instance.new("IntValue")
						placer.Name = "Check"
						placer.Value = 20
						placer.Parent = _G.med[i]
					end 
					if (playerlist[m].Character.Torso.Position - _G.med[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and _G.med[i]:findFirstChild("Check") then
			_G.med[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

