function Anchor(object)
	local despawnthingy = Instance.new("IntValue")
	despawnthingy.Value = 20
	despawnthingy.Name = "DespawnThingy"
	despawnthingy.Parent = object
end

function DespawnLoot()
	local pile = Game.Workspace.Loot:GetChildren()
	for r = 1, #pile do
		local lootdistcheck = 0
		local headrest = pile[r]:findFirstChild("Head", true)
		if headrest then
			local pl = Game.Players:GetChildren()
			for t = 1, #pl do
				if pl[t].Character then
					if pl[t].Character:findFirstChild("Torso") then
						if (headrest.Position - pl[t].Character.Torso.Position).magnitude < lootdistance then
							lootdistcheck = 1
						end
					end
				end
			end
			if lootdistcheck == 0 then
				pile[r]:Destroy()
			end 
		end
	end
end 

