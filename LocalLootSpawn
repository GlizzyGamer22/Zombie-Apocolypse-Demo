						elseif ting == 582 then
							model = backpack.Holo:clone()
						elseif ting == 583 then
							model = backpack.Reflex:clone()
						elseif ting == 584 then
							model = backpack.Kobra:clone()
						elseif ting == 585 then
							model = backpack.Grip:clone()
						elseif ting == 586 then
							model = backpack.Laser:clone()
						elseif ting >= 587 and ting < 589 then
							model = backpack.Suppressor9:clone()
						elseif ting == 589 then
							model = backpack.Suppressor45:clone()
						elseif ting == 590 then
							model = backpack.Suppressor556:clone()
						elseif ting == 591 then
							model = backpack.Suppressor762:clone()
						elseif ting == 592 then
							model = backpack.Suppressor545:clone()
						elseif ting == 593 then
							model = backpack.FlashlightAttachment:clone()
						elseif ting == 594 then
							model = backpack.ACOG:clone()
						elseif ting == 595 then
							model = backpack.SUSAT:clone()
						elseif ting >= 600 and ting < 606 then
							model = backpack.FlashlightMilitary:clone()
						elseif ting >= 610 and ting < 615 then
							model = backpack.OmniLight:clone()
						elseif ting >= 620 and ting < 626 then
							model = backpack.RedChemlight:clone()
						elseif ting >= 630 and ting < 636 then
							model = backpack.GreenChemlight:clone()
						elseif ting >= 640 and ting < 646 then
							model = backpack.BlueChemlight:clone()
						elseif ting >= 650 and ting < 656 then
							model = backpack.RoadFlare:clone()
						elseif ting >= 660 and ting < 683 then
							model = backpack.AKAmmo30:clone()
						elseif ting >= 683 and ting < 690 then
							model = backpack.AKAmmo45:clone()
						elseif ting >= 690 and ting < 692 then
							model = backpack.AKAmmo75:clone()
						elseif ting >= 692 and ting < 695 then
							model = backpack.Detonator:clone()
						elseif ting >= 695 and ting < 700 then
							model = backpack.C4:clone()
						elseif ting >= 700 and ting < 705 then
							model = backpack.TM46:clone()
						elseif ting >= 705 and ting < 711 then
							model = backpack.VS50:clone()
						elseif ting >= 712 and ting < 720 then
							model = backpack.Radio:clone()
						elseif ting >= 720 and ting < 723 then
							local tiert = math.random(1,6)
							if tiert == 1 then
								model = backpack.CamoDesertBottom:clone()
							elseif tiert == 2 then
								model = backpack.CamoUrbanBottom:clone()
							elseif tiert == 3 then
								model = backpack.CamoJungleBottom:clone()
							elseif tiert == 4 then
								model = backpack.CamoWoodlandsBottom:clone()
							elseif tiert == 5 then
								model = backpack.CamoSnowBottom:clone()
							else
								model = backpack.CamoSpecialBottom:clone()
							end
						elseif ting >= 723 and ting < 733 then
							local tiert = math.random(1,73)
							if tiert >= 1 and tiert < 11 then
								model = backpack.BallisticDesert:clone()
							elseif tiert >= 11 and tiert < 21 then
								model = backpack.BallisticJungle:clone()
							elseif tiert >= 21 and tiert < 31 then
								model = backpack.BallisticUrban:clone()
							elseif tiert >= 31 and tiert < 40 then
								model = backpack.PilotGreen:clone()
							elseif tiert >= 40 and tiert < 49 then
								model = backpack.PilotBlack:clone()
							elseif tiert >= 49 and tiert < 58 then
								model = backpack.RedBeret:clone()
							elseif tiert >= 58 and tiert < 66 then
								model = backpack.MaskSpecOps:clone()
							elseif tiert >= 66 and tiert < 74 then
								model = backpack.Firefighter:clone()
							end
						elseif ting >= 733 and ting < 745 then
							model = backpack.CrateLow:clone()
						elseif ting >= 745 and ting < 754 then
							model = backpack.CrateMed:clone()
						elseif ting >= 754 and ting < 759 then
							model = backpack.CrateHigh:clone()
						end
						if model then
							local g = model:GetChildren()
							for d = 1, #g do
								if g[d].className == "Model" then
									g[d]:TranslateBy((_G.mgen[i].Position - g[d].Head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
									local m = g[d]:GetChildren()
									for x = 1, #m do
										if m[x].className == "Part" then
											m[x].Velocity = Vector3.new(0,0,0)
										end
									end
								end
							end 
							model.Parent = Game.Workspace.Loot
						end
						zing = zing + 1
						until zing == 4
						zing = 0
						placer = Instance.new("IntValue")
						placer.Name = "Check"
						placer.Value = 20
						placer.Parent = _G.mgen[i]
					end 
					if (playerlist[m].Character.Torso.Position - _G.mgen[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and _G.mgen[i]:findFirstChild("Check") then
			_G.mgen[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

function BuildingMaterialLootCheck()
	for i = 1, #_G.buildmat do
		local distancecheck = 0
		for m = 1, #playerlist do 
			if playerlist[m].Character then
				if playerlist[m].Character:findFirstChild("Torso") then
					if (playerlist[m].Character.Torso.Position - _G.buildmat[i].Position).magnitude < spawndistance and _G.buildmat[i]:findFirstChild("Check") == nil then
						ting = math.random(1,100)
						model = nil
						if ting >= 1 and ting < 5 then
							model = backpack.Material1:clone()
						elseif ting >= 5 and ting < 10 then
							model = backpack.Material2:clone()
						elseif ting >= 10 and ting < 15 then
							model = backpack.Material3:clone()
						elseif ting >= 15 and ting < 20 then
							model = backpack.Material4:clone()
						elseif ting >= 20 and ting < 25 then
							model = backpack.Material5:clone()
						elseif ting >= 25 and ting < 30 then
							model = backpack.Material6:clone()
						elseif ting >= 30 and ting < 33 then
							model = backpack.Floodlight:clone()
						elseif ting >= 33 and ting < 36 then
							model = backpack.SmallCrate:clone()
						elseif ting >= 36 and ting < 38 then
							model = backpack.LargeCrate:clone()
						end
						if model then
							local g = model:GetChildren()
							for d = 1, #g do
								if g[d].className == "Model" then
									g[d]:TranslateBy((_G.buildmat[i].Position - g[d].Head.Position) + Vector3.new(math.random(-3,3), math.random(1,3), math.random(-3,3)))
									local m = g[d]:GetChildren()
									for x = 1, #m do
										if m[x].className == "Part" then
											m[x].Velocity = Vector3.new(0,0,0)
										end
									end
								end
							end 
							model.Parent = Game.Workspace.Loot
						end
						placer = Instance.new("IntValue")
						placer.Name = "Check"
						placer.Value = 20
						placer.Parent = _G.buildmat[i]
					end 
					if (playerlist[m].Character.Torso.Position - _G.buildmat[i].Position).magnitude < resetdistance then 
						distancecheck = 1
					end
				end 
			end 
		end
		if distancecheck == 0 and _G.buildmat[i]:findFirstChild("Check") then
			_G.buildmat[i]:findFirstChild("Check"):Destroy()
		end 
	end
end

