wait(5)
math.randomseed(tick())

backpack = Game.Lighting.LootDrops
model = nil
spawndistance = 600
lootdistance = 1195
resetdistance = 1200
playerlist = nil
zing = 0

if not _G.med then
	_G.med = Game.Workspace.LootSpawners.MedicalSpawners:GetChildren()
	_G.mwep = Game.Workspace.LootSpawners.MilitaryWSpawners:GetChildren()
	_G.buildmat = Game.Workspace.LootSpawners.BuildingMaterialSpawners:GetChildren()
	_G.mgen = Game.Workspace.LootSpawners.MilitaryGSpawners:GetChildren()
	_G.vparts = Game.Workspace.LootSpawners.VehiclePartSpawners:GetChildren()
	
	_G.wepgroup1 = Game.Workspace.LootSpawners.WeaponSpawners.Weapon1Spawners:GetChildren()
	_G.wepgroup2 = Game.Workspace.LootSpawners.WeaponSpawners.Weapon2Spawners:GetChildren()
	
	_G.gengroup1 = Game.Workspace.LootSpawners.GenericSpawners.Generic1Spawners:GetChildren()
	_G.gengroup2 = Game.Workspace.LootSpawners.GenericSpawners.Generic2Spawners:GetChildren()
	_G.gengroup3 = Game.Workspace.LootSpawners.GenericSpawners.Generic3Spawners:GetChildren()

	for mo = 1, #_G.med do
		_G.med[mo]:Destroy()
	end
	
	for mo = 1, #_G.mwep do
		_G.mwep[mo]:Destroy()
	end
	
	for mo = 1, #_G.buildmat do
		_G.buildmat[mo]:Destroy()
	end
	
	for mo = 1, #_G.mgen do
		_G.mgen[mo]:Destroy()
	end
	
	for mo = 1, #_G.vparts do
		_G.vparts[mo]:Destroy()
	end
	
	for mo = 1, #_G.wepgroup1 do
		_G.wepgroup1[mo]:Destroy()
	end
	
	for mo = 1, #_G.wepgroup2 do
		_G.wepgroup2[mo]:Destroy()
	end
	
	for mo = 1, #_G.gengroup1 do
		_G.gengroup1[mo]:Destroy()
	end
	
	for mo = 1, #_G.gengroup2 do
		_G.gengroup2[mo]:Destroy()
	end
	
	for mo = 1, #_G.gengroup3 do
		_G.gengroup3[mo]:Destroy()
	end
end

